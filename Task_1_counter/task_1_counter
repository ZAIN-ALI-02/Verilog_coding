#! /usr/bin/vvp
:ivl_version "10.3 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 9;
:vpi_module "system";
:vpi_module "vhdl_sys";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x5604ac103c40 .scope module, "task_1_counter_tb" "task_1_counter_tb" 2 4;
 .timescale -9 -9;
v0x5604ac11a750_0 .var "clk_tb", 0 0;
v0x5604ac11a810_0 .var "cnt_in_tb", 3 0;
v0x5604ac11a8e0_0 .net "cnt_out_tb", 3 0, v0x5604ac11a340_0;  1 drivers
v0x5604ac11a9e0_0 .var "enab_tb", 0 0;
v0x5604ac11aab0_0 .var "load_tb", 0 0;
v0x5604ac11aba0_0 .var "rst_tb", 0 0;
S_0x5604ac103dc0 .scope module, "uut" "task_1_counter" 2 14, 3 1 0, S_0x5604ac103c40;
 .timescale -9 -9;
    .port_info 0 /INPUT 1 "clk"
    .port_info 1 /INPUT 1 "rst"
    .port_info 2 /INPUT 1 "load"
    .port_info 3 /INPUT 1 "enab"
    .port_info 4 /INPUT 4 "cnt_in"
    .port_info 5 /OUTPUT 4 "cnt_out"
v0x5604ac0cd130_0 .net "clk", 0 0, v0x5604ac11a750_0;  1 drivers
v0x5604ac11a260_0 .net "cnt_in", 3 0, v0x5604ac11a810_0;  1 drivers
v0x5604ac11a340_0 .var "cnt_out", 3 0;
v0x5604ac11a400_0 .net "enab", 0 0, v0x5604ac11a9e0_0;  1 drivers
v0x5604ac11a4c0_0 .net "load", 0 0, v0x5604ac11aab0_0;  1 drivers
v0x5604ac11a5d0_0 .net "rst", 0 0, v0x5604ac11aba0_0;  1 drivers
E_0x5604ac0cdb20 .event posedge, v0x5604ac0cd130_0;
    .scope S_0x5604ac103dc0;
T_0 ;
    %wait E_0x5604ac0cdb20;
    %load/vec4 v0x5604ac11a5d0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 4;
    %assign/vec4 v0x5604ac11a340_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x5604ac11a4c0_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.2, 8;
    %load/vec4 v0x5604ac11a260_0;
    %assign/vec4 v0x5604ac11a340_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x5604ac11a400_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.4, 8;
    %load/vec4 v0x5604ac11a340_0;
    %addi 1, 0, 4;
    %assign/vec4 v0x5604ac11a340_0, 0;
T_0.4 ;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x5604ac103c40;
T_1 ;
    %vpi_call 2 6 "$dumpfile", "task_1_counter_tb.vcd" {0 0 0};
    %vpi_call 2 7 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x5604ac103c40 {0 0 0};
    %end;
    .thread T_1;
    .scope S_0x5604ac103c40;
T_2 ;
    %vpi_call 2 24 "$monitor", "time=%0d,reset=%b,load=%b,enable=%b,cnt_in=%d,cnt_out=%d", $time, v0x5604ac11aba0_0, v0x5604ac11aab0_0, v0x5604ac11a9e0_0, v0x5604ac11a810_0, v0x5604ac11a8e0_0 {0 0 0};
    %end;
    .thread T_2;
    .scope S_0x5604ac103c40;
T_3 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5604ac11a750_0, 0, 1;
    %end;
    .thread T_3;
    .scope S_0x5604ac103c40;
T_4 ;
    %delay 5, 0;
    %load/vec4 v0x5604ac11a750_0;
    %inv;
    %store/vec4 v0x5604ac11a750_0, 0, 1;
    %jmp T_4;
    .thread T_4;
    .scope S_0x5604ac103c40;
T_5 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5604ac11aba0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5604ac11aba0_0, 0, 1;
    %delay 50, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5604ac11aba0_0, 0, 1;
    %delay 10, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5604ac11aba0_0, 0, 1;
    %end;
    .thread T_5;
    .scope S_0x5604ac103c40;
T_6 ;
    %delay 18, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5604ac11aab0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5604ac11aab0_0, 0, 1;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5604ac11aab0_0, 0, 1;
    %end;
    .thread T_6;
    .scope S_0x5604ac103c40;
T_7 ;
    %delay 5, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x5604ac11a9e0_0, 0, 1;
    %delay 27, 0;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x5604ac11a9e0_0, 0, 1;
    %end;
    .thread T_7;
    .scope S_0x5604ac103c40;
T_8 ;
    %pushi/vec4 8, 0, 4;
    %store/vec4 v0x5604ac11a810_0, 0, 4;
    %delay 15, 0;
    %pushi/vec4 13, 0, 4;
    %store/vec4 v0x5604ac11a810_0, 0, 4;
    %delay 25, 0;
    %pushi/vec4 15, 0, 4;
    %store/vec4 v0x5604ac11a810_0, 0, 4;
